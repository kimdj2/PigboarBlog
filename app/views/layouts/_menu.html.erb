      <div class="col-sm-3">
          <div class='mainContentsTitle'>
            <h3 class="tde" style="margin-top:1rem;margin-bottom:1rem;">         
                <span>Tags</span>         
            </h3>
          </div>
      <div class="list-group article_content" >
        <%= link_to("/boards", class:"list-group-item list-group-item-action") do %> 
          全体を見る
        <% end %>
        <% @tags.each do |tag| %>
          <%= link_to(tag_path(tag.name), class:"list-group-item list-group-item-action") do %>
            <%=tag.name%><span class="badge badge-dark" style="float:right;"><%=tag.taggings_count%></span>
          <% end %> 
        <%end%>
      </div>
      <br>
      <div class='mainContentsTitle'>
            <h3 class="tde" style="margin-top:1rem;margin-bottom:1rem;">     
                <span>Archive</span>             
            </h3>
      </div>
      <div class="list-group article_content">
        <% @board_month.each do |board_month| %>
          <%= link_to(created_path(board_month.month.strftime('%Y-%m')), class:"list-group-item list-group-item-action") do %>
            <%=board_month.month.strftime('%Y-%m')%><span class="badge badge-dark" style="float:right;"><%=board_month.total_month%></span>
          <% end %> 
        <%end%>
      </div>
      <br>
      <div class="card">
        <div class="card-body"> 
          <a class="twitter-timeline" href="https://twitter.com/inoshishi30?ref_src=twsrc%5Etfw">Tweets by inoshishi30</a> <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
      </div>
    </div>